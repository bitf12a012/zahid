    <body background="~/images/wxp%20(30).jpg" class="img-responsive">
        <nav class="navbar navbar-default">
            <div class="container-fluid">
                <div class="navbar-header">
                    <button type="button" class="navbar-toggle collapsed" data-toggle="collapse" data-target="#bs-example-navbar-collapse-2">
                        <span class="sr-only">Toggle navigation</span>
                        <span class="icon-bar"></span>
                        <span class="icon-bar"></span>
                        <span class="icon-bar"></span>
                    </button>
                    <a class="navbar-brand" href="/Home/Index">ComputerInfo</a>
                </div>

                <div class="collapse navbar-collapse" id="bs-example-navbar-collapse-2">
                   
                    <div class="navbar-form navbar-left">
                    @using (Html.BeginForm("signIn", "Admin", FormMethod.Post)){

                        @Html.TextBoxFor(model => model.Email, new { placeholder = "Email", type = "email" })
                        @Html.TextBoxFor(model => model.Passward, new { placeholder = "passward", type = "text" })

                        <button type="submit"><i class="icon-hand-right"></i> SignIn</button>
                    }



                </div>
                        </div>


        </nav>

    @RenderBody();
